{
  "version": 2,
  "builds": [
    {
      "src": "src/handler.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "methods": ["POST"],
      "dest": "/src/handler.js"
    },
    {
      "src": "/register",
      "methods": ["POST"],
      "dest": "/src/handler.js"
    },
    {
      "src": "/handler",
      "methods": ["GET"],
      "dest": "src/handler.js"
    }
  ]
}
